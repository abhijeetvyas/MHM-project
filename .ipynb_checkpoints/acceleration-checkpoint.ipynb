    "def acceleration(l,lc,aplha,omega,theta,n):\n",
    "    r=l-lc\n",
    "    for i in range(1,n):\n",
    "        ax[i]=-(r*alpha[i]*sin(theta[i])+omega[i]*omega[i]*r*cos(theta[i]))\n",
    "        ay[i]=r*alpha[i]*cos(theta[i])-omega[i]*omega[i]*r*sin(theta[i])\n",
    "    return [ax,ay]\n"
   ]
