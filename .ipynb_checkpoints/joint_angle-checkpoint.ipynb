    "def jointangle(pt1x,pt1y,pt2x,pt2y,pt3x,pt3y,n):\n",
    "    for i in range(0,n):\n",
    "        m1[i]=(pt2y[i]-pt1y[i])/(pt2x[i]-pt1x[i])\n",
    "        m2[i]=(pt3y[i]-pt1y[i])/(pt3x[i]-pt1x[i])\n",
    "        angle[i]=atan((m2[i]-m1[i])/(1+m1[i]*m2[i]))\n",
    "    return angle\n"
   ]
