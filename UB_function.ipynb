    "    for i in range(1,n):    \n",
    "        ax[i]=-F1x[i]/mB\n",
    "        ay[i]=-(mB*9.81+F1y[i])/mB\n",
    "        alpha[i]=-M1[i]/(I+mB*l*l)\n",
    "    return ax,ay,alpha"
